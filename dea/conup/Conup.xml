<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE conup [
<!ENTITY experiment SYSTEM "ExpSetting.xml">
]>
<conup>
	<configuration>
		<algorithms>
			<algorithm enable="yes">CONSISTENCY_ALGORITHM</algorithm>
			<algorithm enable="no">TRANQUILLITY_ALGORITHM</algorithm>
			<algorithm enable="no">QUIESCENCE_ALGORITHM</algorithm>
		</algorithms>
		<freenessStrategies>
			<freenessStrategy enable="no">BLOCKING_FOR_FREENESS</freenessStrategy>
			<freenessStrategy enable="yes">CONCURRENT_VERSION_FOR_FREENESS</freenessStrategy>
			<freenessStrategy enable="no">WAITING_FOR_FREENESS</freenessStrategy>
		</freenessStrategies>
	</configuration>


	<staticDeps>
		 
 
		 
		
		<component name="PortalComponent" collect_port="8003">
			<child>AuthComponent</child>
			<child>ProcComponent</child>
		</component>
	
		<component name="ProcComponent" collect_port="8002">
			<parent>PortalComponent</parent>
			 
			<child>DBComponent</child>
			 
			<child>AuthComponent</child>
			
		</component>
		
		<component name="AuthComponent" collect_port="8000">
		 	<!-- <parent>PortalComponent</parent> 
			<parent>ProcComponent</parent>  -->
		</component>
		
		 
		
		<component name="HelloworldComponent" collect_port="8000">
			  <parent>CallComponent</parent>
			   <parent>PaPaComponent</parent>     
		</component>
		
		 <component name="DBComponent" collect_port="8001">
			  
	     </component> 
		
		<component name="CallComponent" collect_port="8002">
			  <parent>PaPaComponent</parent>   
			<child>HelloworldComponent</child>
			<!-- <child>DBComponent</child> -->
		</component>
		
		<component name="PaPaComponent" collect_port="8003">
			<child>CallComponent</child>
			<child>HelloworldComponent</child>	
		</component>
	</staticDeps>

</conup>
